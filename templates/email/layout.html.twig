<body bgcolor="#ffffff" text="#414141" link="#666666" yahoo="fix" style="background-color: #ffffff;">
<div class="preview-text" style="color: #ffffff; display: none; font-size: 0; line-height: 0; max-height: 0; max-width: 0; opacity: 0; overflow: hidden;">
    {% block preview %}{% endblock %}
</div>

<table cellspacing="0" cellpadding="0" border="0" role="presentation" class="nl2go-body-table" width="100%"
       style="background-color: #ffffff; width: 100%;">
    <tr>
        <td>
            <table cellspacing="0" cellpadding="0" border="0" role="presentation" width="600" align="center"
                   class="r0-o" style="table-layout: fixed; width: 600px;">
                <tr>
                    <td valign="top" class="r1-i" style="background-color: #ffffff;">
                        <table cellspacing="0" cellpadding="0" border="0" role="presentation" width="100%"
                               align="center" class="r3-o" style="table-layout: fixed; width: 100%;">
                            <tr>
                                <td class="r4-i" style="padding-bottom: 20px; padding-top: 20px;">
                                    <table width="100%" cellspacing="0" cellpadding="0" border="0" role="presentation">
                                        <tr>
                                            <th width="100%" valign="top" class="r5-c" style="font-weight: normal;">
                                                <table cellspacing="0" cellpadding="0" border="0" role="presentation"
                                                       width="100%" class="r6-o"
                                                       style="table-layout: fixed; width: 100%;">
                                                    <tr>
                                                        <td valign="top" class="r7-i"
                                                            style="padding-left: 15px; padding-right: 15px;">
                                                            <table width="100%" cellspacing="0" cellpadding="0"
                                                                   border="0" role="presentation">
                                                                <tr>
                                                                    <td class="r2-c" align="center">
                                                                        <table cellspacing="0" cellpadding="0"
                                                                               border="0" role="presentation"
                                                                               width="199" class="r8-o"
                                                                               style="table-layout: fixed; width: 199px;">
                                                                            <tr>
                                                                                <td class="r9-i"
                                                                                    style="font-size: 0px; line-height: 0px; padding-bottom: 15px; padding-top: 15px;">
                                                                                    <img
                                                                                        src="{{ absolute_url(asset(logo_dark_path, 'app')) }}"
                                                                                        width="199" border="0"
                                                                                        alt="{{ project_name }}"
                                                                                        style="display: block; width: 100%;">
                                                                                </td>
                                                                            </tr>
                                                                        </table>
                                                                    </td>
                                                                </tr>
                                                            </table>
                                                        </td>
                                                    </tr>
                                                </table>
                                            </th>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </table>

                        {% block content %}
                            <table cellspacing="0" cellpadding="0" border="0" role="presentation" width="100%"
                                   align="center" class="r3-o" style="table-layout: fixed; width: 100%;">
                                <tr>
                                    <td class="r11-i" style="padding-bottom: 40px; padding-top: 20px;">
                                        <table width="100%" cellspacing="0" cellpadding="0" border="0" role="presentation">
                                            <tr>
                                                <th width="100%" valign="bottom" class="r12-c" style="font-weight: normal;">
                                                    <table cellspacing="0" cellpadding="0" border="0" role="presentation"
                                                           width="100%" class="r6-o"
                                                           style="table-layout: fixed; width: 100%;">
                                                        <tr>
                                                            <td valign="top" class="r7-i"
                                                                style="padding-left: 15px; padding-right: 15px;">
                                                                <table width="100%" cellspacing="0" cellpadding="0"
                                                                       border="0" role="presentation">
                                                                    <tr>
                                                                        <td class="r13-c" align="left">
                                                                            <table cellspacing="0" cellpadding="0"
                                                                                   border="0" role="presentation"
                                                                                   width="100%" class="r14-o"
                                                                                   style="table-layout: fixed; width: 100%;">
                                                                                <tr>
                                                                                    <td align="center" valign="top"
                                                                                        class="r15-i nl2go-default-textstyle"
                                                                                        style="color: #414141; font-family: Arial; font-size: 16px; word-break: break-word; line-height: 1; text-align: center;">
                                                                                        <div>
                                                                                            <h2 class="default-heading2" style="margin: 0; color: #414141; font-family: Arial; font-size: 32px; word-break: break-word;">
                                                                                                TITRE
                                                                                            </h2>
                                                                                        </div>
                                                                                    </td>
                                                                                </tr>
                                                                            </table>
                                                                        </td>
                                                                    </tr>
                                                                </table>
                                                            </td>
                                                        </tr>
                                                    </table>
                                                </th>
                                            </tr>
                                        </table>
                                    </td>
                                </tr>
                            </table>
                            <table cellspacing="0" cellpadding="0" border="0" role="presentation" width="100%"
                                   align="center" class="r3-o" style="table-layout: fixed; width: 100%;">
                                <tr>
                                    <td class="r4-i" style="padding-bottom: 20px; padding-top: 20px;">
                                        <table width="100%" cellspacing="0" cellpadding="0" border="0" role="presentation">
                                            <tr>
                                                <th width="100%" valign="top" class="r5-c" style="font-weight: normal;">
                                                    <table cellspacing="0" cellpadding="0" border="0" role="presentation"
                                                           width="100%" class="r6-o"
                                                           style="table-layout: fixed; width: 100%;">
                                                        <tr>
                                                            <td valign="top" class="r7-i"
                                                                style="padding-left: 15px; padding-right: 15px;">
                                                                <table width="100%" cellspacing="0" cellpadding="0"
                                                                       border="0" role="presentation">
                                                                    <tr>
                                                                        <td class="r16-c nl2go-default-textstyle"
                                                                            align="left"
                                                                            style="color: #414141; font-family: Arial; font-size: 16px; line-height: 1.5; word-break: break-word; text-align: left; valign: top;">
                                                                            <div>
                                                                                <p style="margin: 0;">TEXTE</p>
                                                                            </div>
                                                                        </td>
                                                                    </tr>
                                                                </table>
                                                            </td>
                                                        </tr>
                                                    </table>
                                                </th>
                                            </tr>
                                        </table>
                                    </td>
                                </tr>
                            </table>
                        {% endblock %}

                        {% block action %}
                            <table cellspacing="0" cellpadding="0" border="0" role="presentation" width="100%"
                                   align="center" class="r3-o" style="table-layout: fixed; width: 100%;">
                                <tr>
                                    <td class="r4-i" style="padding-bottom: 20px; padding-top: 20px;">
                                        <table width="100%" cellspacing="0" cellpadding="0" border="0" role="presentation">
                                            <tr>
                                                <th width="100%" valign="top" class="r5-c" style="font-weight: normal;">
                                                    <table cellspacing="0" cellpadding="0" border="0" role="presentation"
                                                           width="100%" class="r6-o"
                                                           style="table-layout: fixed; width: 100%;">
                                                        <tr>
                                                            <td valign="top" class="r17-i"
                                                                style="padding-left: 15px; padding-right: 15px; padding-top: 50px;">
                                                                <table width="100%" cellspacing="0" cellpadding="0"
                                                                       border="0" role="presentation">
                                                                    <tr>
                                                                        <td class="r18-c" align="center"
                                                                            style="align: center; padding-bottom: 0px; padding-top: 0px; valign: top;">
                                                                            <table cellspacing="0" cellpadding="0"
                                                                                   border="0" role="presentation"
                                                                                   width="285" class="r19-o"
                                                                                   style="background-color: #222222; border-collapse: separate; border-color: #222222; border-radius: 4px; border-style: solid; border-width: 0px; table-layout: fixed; width: 285px;">
                                                                                <tr>
                                                                                    <td height="18" align="center"
                                                                                        valign="top"
                                                                                        class="r20-i nl2go-default-textstyle"
                                                                                        style="word-break: break-word; background-color: #222222; border-radius: 4px; color: #ffffff; font-family: Arial; font-size: 16px; font-style: normal; line-height: 1.15; padding-bottom: 12px; padding-top: 12px; text-align: center;">
                                                                                        <a href="#top"
                                                                                           class="r21-r default-button"
                                                                                           target="_blank" data-btn="1"
                                                                                           style="font-style: normal; font-weight: normal; line-height: 1.15; text-decoration: none; word-break: break-word; word-wrap: break-word; display: block; -webkit-text-size-adjust: none; color: #ffffff; font-family: Arial; font-size: 16px;">
                                                                                            <span>ACTION</span></a>
                                                                                    </td>
                                                                                </tr>
                                                                            </table>
                                                                        </td>
                                                                    </tr>
                                                                </table>
                                                            </td>
                                                        </tr>
                                                    </table>
                                                </th>
                                            </tr>
                                        </table>
                                    </td>
                                </tr>
                            </table>
                        {% endblock %}

                        <table cellspacing="0" cellpadding="0" border="0" role="presentation" width="100%"
                               align="center" class="r3-o" style="table-layout: fixed; width: 100%;">
                            <tr>
                                <td class="r22-i" style="padding-bottom: 20px; padding-top: 80px;">
                                    <table width="100%" cellspacing="0" cellpadding="0" border="0" role="presentation">
                                        <tr>
                                            <th width="100%" valign="top" class="r5-c" style="font-weight: normal;">
                                                <table cellspacing="0" cellpadding="0" border="0" role="presentation"
                                                       width="100%" class="r6-o"
                                                       style="table-layout: fixed; width: 100%;">
                                                    <tr>
                                                        <td valign="top" class="r7-i"
                                                            style="padding-left: 15px; padding-right: 15px;">
                                                            <table width="100%" cellspacing="0" cellpadding="0"
                                                                   border="0" role="presentation">
                                                                <tr>
                                                                    <td class="r13-c" align="left">
                                                                        <table cellspacing="0" cellpadding="0"
                                                                               border="0" role="presentation"
                                                                               width="119" class="r23-o"
                                                                               style="table-layout: fixed; width: 119px;">
                                                                            <tr>
                                                                                <td class="r9-i"
                                                                                    style="font-size: 0px; line-height: 0px; padding-bottom: 15px; padding-top: 15px;">
                                                                                    <img
                                                                                        src="{{ absolute_url(asset(logo_dark_path, 'app')) }}"
                                                                                        width="119" border="0"
                                                                                        alt="{{ project_name }}"
                                                                                        style="display: block; width: 100%;">
                                                                                </td>
                                                                            </tr>
                                                                        </table>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <td class="r24-c nl2go-default-textstyle"
                                                                        align="left"
                                                                        style="color: #414141; font-family: Arial; font-size: 16px; line-height: 1.5; word-break: break-word; padding-bottom: 10px; text-align: left; valign: top;">
                                                                        <div><p style="margin: 0;"><a
                                                                                    href="{{ absolute_url(path('app_profil')) }}"
                                                                                    style="color: #666; text-decoration: underline;">{{ 'user.profil.view'|trans }}</a>
                                                                            </p></div>
                                                                    </td>
                                                                </tr>
                                                            </table>
                                                        </td>
                                                    </tr>
                                                </table>
                                            </th>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>
</body>
</html>
